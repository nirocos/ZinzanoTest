<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/lanceCgTitle"
        android:text="@string/tvAdjustablelance"
        android:textColor="#ffee00"
        android:gravity="center"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataPressure"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataFlowRate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataVoltage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataCurrent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataPower"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.faprun.zinzanotest.view.InputDataCustomViewGroup
        android:id="@+id/cgDataPowerFactor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>